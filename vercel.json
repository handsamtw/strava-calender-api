{
      "rewrites": [
        { "source": "/(.*)", "destination": "/api/index" }
      ],
      
      "functions": {
        "api/**": {
          "maxDuration": 60,
          "memory": 3008,
          "excludeFiles": "{.next,*.cache,node_modules,public,app}/**"
        }
      },
      "routes": [
        {
          "src": "api/*",
          "headers": { "Cache-Control": "max-age=60" } 
        }
      ]

}